{
  "name": "talenet",
  "version": "1.0.0",
  "description": "TALEnet â€“ intergalactic skill & idea sharing community",
  "author": "Andreas Baldeau <andreas@baldeau.net>, Henry Bubert <cryptix@riseup.net>, Torben Spieker <torben.spieker@posteo.de>, Kathia von Roth <stephiewunder@posteo.de>, Sascha Wollin <saschawollin@gmail.com>",
  "private": true,
  "main": "./dist/electron/index.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "e2e": "npm run pack && mocha test/e2e",
    "dev": "node .electron-vue/dev-runner.js",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src test/unit/specs test/e2e/specs",
    "lint:fix": "eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter --fix src test/unit/specs test/e2e/specs",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "npm run lint:fix"
  },
  "build": {
    "productName": "talenet",
    "appId": "net.tale.client",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons",
      "category": "P2P"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-vue": "^1.0.0-beta.7",
    "electron-default-menu": "^1.0.1",
    "electron-window-state": "^4.1.1",
    "fix-path": "^2.1.0",
    "pull-stream": "^3.6.1",
    "scuttlebot": "^10.4.10",
    "ssb-about": "^0.1.0",
    "ssb-backlinks": "^0.4.1",
    "ssb-client": "^4.5.1",
    "ssb-friends": "^2.3.2",
    "ssb-marked": "^0.7.2",
    "ssb-private": "^0.1.4",
    "ssb-query": "^0.1.2",
    "vee-validate": "^2.0.0-beta.17",
    "vue": "^2.5.2",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^7.3.2",
    "vue-router": "^2.8.1",
    "vuex": "^2.4.1"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "avoriaz": "^4.2.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "cfonts": "^1.1.3",
    "chai": "^4.0.0",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^1.8.1",
    "electron-builder": "^19.43.3",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.1",
    "eslint": "^4.10.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "inject-loader": "^3.0.0",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-electron": "^5.1.1",
    "karma-mocha": "^1.3.0",
    "karma-sinon-chai": "^1.3.3",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.31",
    "karma-webpack": "^2.0.2",
    "mocha": "^3.5.3",
    "multispinner": "^0.2.1",
    "nightwatch": "^0.9.12",
    "node-loader": "^0.6.0",
    "node-sass": "^4.5.3",
    "require-dir": "^0.3.0",
    "sass-loader": "^6.0.6",
    "semver": "^5.3.0",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.14.0",
    "spectron": "^3.7.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.3",
    "vue-template-compiler": "^2.5.2",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.9.3",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-merge": "^4.1.0"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
